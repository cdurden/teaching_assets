    "CommonMistake": {
        "onload": "MathJax.typeset();",
        "background_html": "<div class=\"row\">\n     <div class=\"col-12\" style=\"text-align: left; line-spacing: 3em;\">\n         <h3>Common Mistake</h3>\n         A common mistake is writing an incorrect sign for the exponent in scientific notation. You need to remember the following facts.\n         <ol>\n             <li>\n                 A <b style=\"font-color: blue;\">positive exponent</b> is used to represent a number <b style=\"font-color: blue;\">greater than 1</b>.\n             </li>\n             <li>\n                 A <b style=\"font-color: red;\">negative exponent</b> is used to represent a number <b style=\"font-color: red;\">greater than 1</b>.\n             </li>\n         </ol>\n     </div>\n</div>\n\n",
        "resources": {
            "slide": "slides/DividingNumbersInScientificNotation/CommonMistake.html"
        },
        "title": "Dividing Numbers In Scientific Notation"
    },
    "PowersOfTen": {
        "onload": "MathJax.typeset();",
        "background_html": "<div class=\"row\">\n     <div class=\"col-12 md-font\" style=\"text-align: left; line-spacing: 3em;\">\n         <h3>Exercise 1</h3>\n         Fill in the table with the different representations of the powers of $10$.\n         <table class=\"md-font\">\n             <tr>\n                 <th>\n                     Exponent form \n                 </th>\n                 <th class=\"sm-font\" style=\"font-size: 18pt;\">\n                     Rewrite with positive exponents\n                 </th>\n                 <th>\n                     Expanded form\n                 </th>\n                 <th>\n                     Fraction form\n                 </th>\n                 <th>\n                     Decimal form\n                 </th>\n                 <th class=\"sm-font\" style=\"font-size: 18pt;\">\n                     $> 1$ or $< 1$?\n                 </th>\n             </tr>\n             <tr>\n                 <td>\n                     $10^5$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^4$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^3$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^2$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^1$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^0$\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td style=\"background-color: #000;\">\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^{-1}$\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^{-2}$\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^{-3}$\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n             <tr>\n                 <td>\n                     $10^{-4}$\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n                 <td>\n                 </td>\n             </tr>\n         </table>\n     </div>\n</div>\n\n",
        "resources": {
            "slide": "slides/DividingNumbersInScientificNotation/PowersOfTen.html"
        },
        "title": "Dividing Numbers In Scientific Notation"
    },
