    "FormsOfEquations": {
        "onload": "MathJax.typeset();",
        "background_html": "<div class=\"row\" style=\"margin-top: 0em;\">\n<div class=\"col-4\">\n    <h5>Slope-intercept form</h5>\n<br/>\n    $$y=mx+b$$<br/><br/><br/>\n\n    $m: $ slope $\\frac{\\text{rise}}{\\text{run}}$\n    $b: y$-intercept\n</div>\n<div class=\"col-4\">\n    <h5>Standard form</h5>\n    <br/><br/>\n    $$Ax + By = C$$\n</div>\n<div class=\"col-4\">\n    <h5>Point-slope form</h5>\n<br/>\n    $$y - y_1 = m(x-x_1)$$<br/><br/><br/>\n    $m: $ slope\n    $(x_1,y_1): $ a point on the line\n</div>\n</div>\n",
        "resources": {
            "slide": "slides/EquationsOfLinesReview/FormsOfEquations.html"
        },
        "title": "Equations Of Lines Review"
    },
